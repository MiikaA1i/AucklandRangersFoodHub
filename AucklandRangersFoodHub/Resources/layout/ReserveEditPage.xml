<?xml version="1.0" encoding="utf-8" ?> 
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@android:color/white"
    android:layout_height="match_parent">
    <!--Auckland rangers text-->
    <TextView
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="Auckland Rangers"
        android:textSize="25dp"
        android:layout_x="100dp"
        android:layout_y="40dp"
    />

          <!--Profile Icon button-->
    <ImageButton
        android:background="@android:color/white"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_x="340dp"
        android:layout_y="15dp"
        android:scaleType="fitCenter"
        android:src="@drawable/profileicon"
        android:id="@+id/ButtonProfileIcon" />


         <!--Back Icon button-->
    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_x="10dp"
        android:layout_y="20dp"
        android:background="@android:color/white"
        android:scaleType="fitCenter"
        android:id="@+id/arrowicon" 
        android:src="@drawable/backbutton"/>

    <!--Reservations Text-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reservations"
        android:textColor="#ff000000"
        android:textSize="25dp"
        android:layout_x="120dp"
        android:layout_y="100dp"/>
    <!--Listview text-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your reservations"
        android:textColor="#ff000000"
        android:layout_x="135dp"
        android:layout_y="150dp"/>
    <!--Listview-->
    <ListView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_x="100dp"
        android:layout_y="200dp"
        android:textColor="#ff000000"
        android:background="#ffe0e0e0"
        android:id="@+id/listViewReservations"/>
    <!--Edit button-->
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="edit"
        android:id="@+id/buttonEdit"
        android:textColor="@android:color/white"
        android:background="@android:color/black"
        android:layout_x="90dp"
        android:layout_y="450dp"
        />
    <!--Add button-->
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="add"
        android:id="@+id/reservationsAddButton"
        android:textColor="@android:color/white"
        android:background="@android:color/black"
        android:layout_x="225dp"
        android:layout_y="450dp"/>
    <!--Menu button-->
    <Button
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:text="@string/Menu_Button"
        android:layout_x="10dp"
        android:layout_y="600dp"
        android:textColor="#ff000000"
        android:background="@drawable/roundbutton"
        style="?android:attr/borderlessButtonStyle"
        android:textSize="15dp"
        android:id="@id/ButtonMenu"/>
    <!--Contact us button-->
    <Button
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:text="@string/Contact_Us_Button"
        android:layout_x="105dp"
        android:layout_y="600dp"
        android:textColor="#ff000000"
        android:background="@drawable/roundbutton"
        style="?android:attr/borderlessButtonStyle"
        android:id="@id/ButtonContactUs"/>
    <!--Profile button-->
    <Button
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:text="@string/Profile_Button"
        android:layout_x="200dp"
        android:layout_y="600dp"
        android:textColor="#ff000000"
        android:background="@drawable/roundbutton"
        style="?android:attr/borderlessButtonStyle"
        android:id="@id/ButtonProfile"/>
    <!--Cart button-->
    <Button
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:text="@string/Cart_button"
        android:layout_x="295dp"
        android:layout_y="600dp"
        android:textColor="#ff000000"
        android:background="@drawable/roundbutton"
        style="?android:attr/borderlessButtonStyle"
        android:id="@id/ButtonCart"/>
</AbsoluteLayout>